<%- include('./partials/header'); -%>
<div class="maincontainer">
	<h1>Home</h1>
	<% blgList.forEach(function(post){ %>
	<h2><%=post.title%></h2>
	<p>
		<%=post.content.substring(0,100)+"..."%><a
			href="/posts/<%=post.title%>"
			>Read more</a
		>
	</p>
	<% });%>
</div>
<%- include('./partials/footer'); -%>
